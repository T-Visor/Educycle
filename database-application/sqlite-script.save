-- Get all participants from Towson University
SELECT Name, Sex, Age FROM Students
UNION ALL
SELECT Name, Sex, Age FROM Faculty_Staff
ORDER BY Name;

-- Get all information on Baltimore Public County Schools
SELECT * FROM BPC;

-- Insert a new Baltimore Public County School
INSERT INTO BPC(School_Name, Person_of_contact, Address) VALUES (?, ?, ?);

-- Get service requests
SELECT MMSR_ID, Priority_level, Departmment, StartDate, EndDate FROM Service_Request;

