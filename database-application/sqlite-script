-- Insert a new Baltimore Public County School
INSERT INTO BPC(School_Name, Person_of_contact, Address) VALUES (?, ?, ?);

-- Get service requests
SELECT MMSR_ID, Priority_level, Departmment, StartDate, EndDate FROM Service_Request;

