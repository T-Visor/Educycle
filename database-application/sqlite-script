-- Get all participants from Towson University
SELECT Name, Sex, Age FROM Students
UNION ALL
SELECT Name, Sex, Age FROM Faculty_Staff
ORDER BY Name;

-- Get all information on Baltimore Public County Schools... once implemented add an update page
